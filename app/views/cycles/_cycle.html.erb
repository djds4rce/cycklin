<li class="isotope-item <%=cycle.brand %> <%=cycle.cycle_type %>">
  <div class="grid-cycle">
    <a id='<%=cycle.id%>' href='<%=cycle.url%>'>
      <img src='<%= cycle.image_url %>' \>
      <div class="cycle-info">
        <p>Name: <%= cycle.name %></p>
        <p>Type: <%=cycle.cycle_type %></p>
        <p>Brand: <%=cycle.brand %></p>
        <p>Price: Rs.<%=cycle.price %></p>        
      </div>
    </a>
  </div>
</li>


